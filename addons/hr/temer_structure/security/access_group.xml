                <field name="name">Developer Admin </field>
                <field name="category_id" ref="base.module_category_hidden"/>
        </record>
  
        <record id="access_property_system_admin_group" model="res.groups">
                <field name="name">System Admin </field>
                <field name="category_id" ref="base.module_category_hidden"/>
        </record>

        

